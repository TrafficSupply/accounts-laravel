{
    "name": "trafficsupply/accounts-laravel",
    "description": "Accounts Laravel - Package to help with logging into TrafficSupply Accounts.",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "Diana van der Schouw",
            "email": "diana.vds@trafficsupply.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "TrafficSupply\\AccountsLaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TrafficSupply\\AccountsLaravel\\Tests\\": "tests/"
        }
    },
  "extra": {
    "laravel": {
      "providers": [
        "TrafficSupply\\AccountsLaravel\\ServiceProvider"
      ]
    }
  },
    "require": {
        "php": "^8.1 || ^8.2 || ^8.3",
        "illuminate/database": "^10 || ^11",
        "illuminate/http": "^10 || ^11",
        "illuminate/routing": "^10 || ^11",
        "illuminate/support": "^10 || ^11",
        "illuminate/view": "^10 || ^11"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    }
}
